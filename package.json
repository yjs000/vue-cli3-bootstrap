{
  "name": "vue-cli3-bootstrap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "inspect" : "vue-cli-service inspect"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"],
          "extensions": [".js", ".jsx", ".ts", ".tsx", ".vue"]
        }
      }
    },
    "rules": {
      "vue/no-multiple-template-root": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
